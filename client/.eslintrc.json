{
    "extends": ["airbnb","prettier"],
    "plugins":["prettier"],
    "rules": {
        "prettier/prettier":"error",
        "func-names":"off",
        "no-unused-vars":"warn",
        "import/no-duplicates":"warn",
        "react/jsx-filename-extension":"off",
        "react/jsx-props-no-spreading": "off",
        "object-shorthand":"off"
        
    }
}
